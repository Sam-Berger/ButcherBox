{
    "manifest_version": 2,
    "name": "ButcherBox Per Pound Calculator",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["calculations.js", "content.js"]
    }],
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {}
}